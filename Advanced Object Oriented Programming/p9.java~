import java.util.*;

class BackAcct{
	long acc;
	double bal;
	static double rate;
	
	static initRate(double val){
		rate = val;
	}
	static displayRate(){
		System.out.println("Rate : " + rate);
	}
	BackAcct(long acc, double b){
		this.acc = acc;
		this.bal = bal;
	}
	double returnBal(){
		return bal;
	}
	void calcInterest(){
		int interest = bal * rate / 100;
		System.out.println("Interest : " + interest);
	}
}
